{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "react-router",
  "headers": [
    {
      "source": "/(fonts|gallery|products|.*\\.(js|css|png|jpg|svg))$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
